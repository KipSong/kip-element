{
  "compilerOptions": {
    "outDir": "dist",
    "module": "esnext",
    "target": "ES5",
    "declaration": true,
    "jsx": "react",
    "moduleResolution": "node",  // 寻找模块方式
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/**/*.test.tsx",
    "src/**/*.stories.tsx",
  ]
}